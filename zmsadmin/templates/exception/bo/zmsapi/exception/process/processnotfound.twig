{% embed "block/scaffholding/board.twig" with {'class': 'exception'} %}

    {% block board_title %}Es ist ein Fehler aufgetreten.{% endblock %}
    {% block board_body %}
        <h2 class="exceptionData-headline">
            Die Vorgangsnummer existiert nicht{% if sourceData.processId %} (Vorgang {{ sourceData.processId }}){% endif %}
        </h2>
        <p>
            {{ exclamationIcon|raw }} {{ errorPrefix }}: {% trans %}Zu Ihrer Auswahl konnte kein Vorgang gefunden werden. Bitte pr√ºfen Sie die angegebene Vorgangsnummer in der URL.{% endtrans %}
        </p>
        <div class="form-actions">
            <button class="btn button-cancel right" onclick="window.location.href='{{ urlGet('workstation') }}'">Abbrechen</button>
        </div>
    {% endblock %}

{% endembed %}


