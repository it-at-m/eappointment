{
    "name": "bo/zmsticketprinter",
    "description": "Frontend for ticketprinter",
    "license": "EUPL-1.2",
    "authors": [
        {
            "name": "Torsten Kunst",
            "email": "torsten.kunst@berlinonline.de"
        },
        {
