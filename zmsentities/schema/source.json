        "label": {
            "type": "string",
            "default": ""
        },
        "lastChange": {
            "type": "number",
            "description": "unix timestamp of the last import",
            "default": 0
        },
        "providers": {
            "type": "array",
            "items": {
                "$ref": "provider.json"
            }
        },
        "requests": {
            "type": "array",
            "items": {
                "$ref": "request.json"
            }
        },
        "requestrelation": {
            "type": "array",
            "items": {
                "$ref": "requestrelation.json"
            }
