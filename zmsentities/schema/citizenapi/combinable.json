{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": ["object", "array"],
  "patternProperties": {
    "^[0-9]+$": {
      "type": ["object", "array"],
      "patternProperties": {
        "^[0-9]+$": {
          "type": "array",
          "items": {
            "type": "integer"
          }
        }
      },
      "additionalProperties": false,
      "minProperties": 1,
      "maxProperties": 1
    }
  }
}
