{
    "name": "eappointment/zmsentities",
    "description": "Entity definitions for eappoinment",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathias Fischer",
            "email": "mathias.fischer@berlinonline.de"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-curl": ">=0",
        "ext-mbstring": ">=0",
        "ext-json": ">=1.0",
        "ext-pcre": ">=0",
        "eappointment/mellon": "@dev",
        "twig/twig": "3.*",
        "league/html-to-markdown": "^5.0",
        "giggsey/libphonenumber-for-php": "^8.13.55",
        "twig/intl-extra": "^3.4",
        "symfony/translation": "^5.4",
        "symfony/twig-bridge": "^5.4",
        "opis/json-schema": "^2.4",
        "roave/security-advisories": "dev-latest"
    },
    "scripts": {
        "command": "bin/configure"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "BO\\Zmsentities\\": "src/Zmsentities/",
            "BO\\Zmsentities\\Tests\\": "tests/Zmsentities/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "helmich/phpunit-psr7-assert": "4.4.1",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "*",
        "roave/security-advisories": "dev-latest"
    }
}
