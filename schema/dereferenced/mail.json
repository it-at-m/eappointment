                "amendment": "Beispiel Termin",
                "appointments": [
                    {
                        "date": 1447869171,
                        "scope": {
                            "id": 123
                        },
                        "slotCount": 2
                    }
                ],
                "authKey": "abcd",
                "clients": [
                    {
                        "familyName": "Max Mustermann",
                        "email": "max@service.berlin.de",
                        "telephone": "030 115"
                    }
                ],
                "createIP": "145.15.3.10",
                "createTimestamp": 1447931596,
                "lastChange": 1447931596,
                "id": 123456,
                "queue": {
                    "arrivalTime": 1447922893,
                    "callCount": 1,
                    "callTime": 1447922938,
