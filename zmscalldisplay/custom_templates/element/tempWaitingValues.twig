<div id="waitingClients" style="display:none">
    {{ waitingClients }}
</div>
<div id="waitingTime" style="display:none">
    {% if scope and scope.status.queue.ghostWorkstationCount == 0 %}
        {{ "%s"|format("unbekannt")|trans }}
    {% else %}
        {% if waitingTime %}
            {% if waitingTime < 120 %}
                {{ waitingTimeOptimistic }} - {{ waitingTime }} {{ "%s"|format("Minuten")|trans }}
            {% else %}
                {{ (waitingTime / 60)|round(0, 'floor') }} {{ "%s"|format("Stunden")|trans }}
            {% endif %}
         {% endif %}
    {% endif %}

</div>

