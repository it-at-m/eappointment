{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RequestVariantListResponse",
  "type": "object",
  "required": ["data", "meta"],
  "properties": {
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "name"],
        "properties": {
          "id":   { "type": "integer" },
          "name": { "type": "string"  }
        },
        "additionalProperties": false
      }
    },
    "meta": {
      "type": "object",
      "required": ["error", "generated", "server"],
      "properties": {
        "error":    { "type": "boolean" },
        "generated":{ "type": "string"  },
        "server":   { "type": "string"  }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
